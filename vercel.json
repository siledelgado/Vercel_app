{
    "builds": [
        {
        "src": "python_heroku_task/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.8.10"}
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "python_heroku_task/wsgi.py"
        }
    ]
}